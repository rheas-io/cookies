{
  "name": "@rheas/cookies",
  "version": "0.0.1",
  "description": "Cookies handling module of the Rheas framework",
  "main": "./index.js",
  "directories": {
    "test": "tests"
  },
  "keywords": [
    "cookies",
    "rheas",
    "framework",
    "express",
    "laravel",
    "server",
    "sessions"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "jest",
    "coverage": "jest --coverage",
